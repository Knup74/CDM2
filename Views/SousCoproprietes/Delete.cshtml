@model CDM.Database.Models.SousCopropriete

@{
    ViewData["Title"] = "Supprimer une sous-copropriété";
}

<h1>Supprimer cette sous-copropriété</h1>

<dl class="row">
    <dt class="col-sm-3">Nom</dt>
    <dd class="col-sm-9">@Model.Nom</dd>
</dl>

<form asp-action="Delete">
    <input type="hidden" asp-for="Id" />
    <button class="btn btn-danger">Supprimer</button>
    <a asp-action="Index" class="btn btn-secondary">Annuler</a>
</form>
